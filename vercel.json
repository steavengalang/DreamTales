{
  "version": 2,
  "name": "dreamtales",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/viewer", "destination": "/viewer.html" },
    { "source": "/DT", "destination": "/DT/index.html" }
  ]
}

